#!/bin/bash
# filepath: debian/prerm

set -e

case "$1" in
    remove|upgrade|deconfigure)
        # Nothing to do - migration handled in postinst
        ;;
    failed-upgrade)
        ;;
    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
        ;;
esac

#DEBHELPER#

exit 0